{
  "name": "@tsparticles/webpack-plugin",
  "version": "2.1.6",
  "main": "dist/webpack-tsparticles.js",
  "types": "dist/webpack-tsparticles.d.ts",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prettify:ci:src": "prettier --check ./src/*",
    "prettify:ci:readme": "prettier --check ./README.md",
    "prettify:src": "prettier --write ./src/*",
    "prettify:readme": "prettier --write ./README.md",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "lint:ci": "eslint src --ext .js,.jsx,.ts,.tsx",
    "compile": "pnpm run build:ts",
    "compile:ci": "pnpm run build:ts",
    "build:ts": "pnpm run build:ts:cjs",
    "build:ts:cjs": "tsc",
    "build": "pnpm run clear:dist && pnpm run prettify:src && pnpm run lint && pnpm run compile && pnpm run prettify:readme",
    "build:ci": "pnpm run clear:dist && pnpm run prettify:ci:src && pnpm run lint:ci && pnpm run compile && pnpm run prettify:ci:readme",
    "clear:dist": "rimraf ./dist",
    "prepack": "pnpm run build"
  },
  "prettier": "@tsparticles/prettier-config",
  "dependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@stylistic/eslint-plugin": "^1.6.3",
    "@tsparticles/eslint-config": "^2.1.6",
    "@tsparticles/prettier-config": "^2.1.6",
    "@types/node": "^20.11.24",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "@types/webpack-env": "^1.18.4",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "babel-loader": "^9.1.3",
    "browserslist": "^4.23.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "prettier": "^3.2.5",
    "prettier-plugin-multiline-arrays": "^3.0.4",
    "rimraf": "^5.0.5",
    "terser-webpack-plugin": "^5.3.10",
    "typescript": "^5.3.3",
    "webpack": "^5.90.3",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4"
  }
}
